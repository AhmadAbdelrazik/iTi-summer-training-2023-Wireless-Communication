#include "DIO_Driver.h"

void DIO_init(uint8 DDRD)
{
	DDRD_REG = DDRD;
}

void DIO_WriteChannel(uint8 PORTD, uint8 bit);
{
	if (bit)
	{
		Set_Bit(PORTD_REG, bit);
	}
	else
	{

	}
}
uint8 DIO_ReadChannel(uint8);
